{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/election", "dest": "/election.html" },
    { "src": "/", "dest": "/election.html" }
  ]
}
