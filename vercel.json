{
  "routes": [
    {
      "src": "/global.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/donors.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "headers": {
        "Content-Type": "text/html"
      }
    }
  ]
}
